CREATE TABLE `dhs`.`custdetails` (
  `cust_id` INT NOT NULL AUTO_INCREMENT,
  `cust_fname` VARCHAR(25) NOT NULL,
  `cust_lname` VARCHAR(25) NOT NULL,
  `cust_house` VARCHAR(25) NOT NULL,
  `cust_city` VARCHAR(25) NOT NULL,
  `cust_dist` VARCHAR(25) NOT NULL,
  `cust_pincode` BIGINT(10) NOT NULL,
  `cust_email` VARCHAR(25) NOT NULL,
  `cust_pass` VARCHAR(25) NOT NULL,
  `cust_phno` BIGINT(10) NOT NULL,
  PRIMARY KEY (`cust_id`));


  CREATE TABLE `dhs`.`service` (
  `s_id` INT NOT NULL AUTO_INCREMENT,
  `s_name` VARCHAR(25) NOT NULL,
  PRIMARY KEY (`s_id`));


CREATE TABLE `dhs`.`sp_details` (
  `sp_id` INT NOT NULL AUTO_INCREMENT,
  `s_id` INT NOT NULL,
  `sp_username` VARCHAR(25) NOT NULL,
  `sp_fname` VARCHAR(25) NOT NULL,
  `sp_lname` VARCHAR(25) NOT NULL,
  `sp_phone` BIGINT(10) NOT NULL,
  `sp_house` VARCHAR(25) NOT NULL,
  `sp_city` VARCHAR(25) NOT NULL,
  `sp_district` VARCHAR(25) NOT NULL,
  `sp_pincode` BIGINT(6) NOT NULL,
  `sp_password` VARCHAR(25) NOT NULL,
  PRIMARY KEY (`sp_id`, `s_id`),
  CONSTRAINT `s_id`
    FOREIGN KEY (`s_id`)
    REFERENCES `dhs`.`service_details` (`s_id`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION);


