CREATE TABLE `dhs`.`request_master` (
  `rm_id` INT NOT NULL AUTO_INCREMENT,
  `cust_id` INT NOT NULL,
  `r_date` DATE NOT NULL,
  PRIMARY KEY (`rm_id`),

  CONSTRAINT `cust_id`
    FOREIGN KEY (`cust_id`)
    REFERENCES `dhs`.`cust_details` (`cust_id`)
);


CREATE TABLE `dhs`.`request_child` (
  `rc_id` INT NOT NULL AUTO_INCREMENT,
  `rm_id` INT NOT NULL,
  `s_id` INT NOT NULL,
  `r_status` VARCHAR(45) NOT NULL,
  `location` INT NOT NULL,
  PRIMARY KEY (`rc_id`),

  CONSTRAINT `rm_id`
    FOREIGN KEY (`rm_id`)
    REFERENCES `dhs`.`request_master` (`rm_id`)
,
  CONSTRAINT `s_id`
    FOREIGN KEY (`s_id`)
    REFERENCES `dhs`.`service_details` (`s_id`)
 );

 CREATE TABLE `dhs`.`accept` (
  `a_id` INT NOT NULL AUTO_INCREMENT,
  `rc_id` INT NOT NULL,
  `sp_id` INT NOT NULL,
  `a_date` DATE NOT NULL,
  `status` VARCHAR(45) NOT NULL,
  PRIMARY KEY (`a_id`),
  CONSTRAINT `rc_id`
    FOREIGN KEY (`rc_id`)
    REFERENCES `dhs`.`request_child` (`rc_id`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION,
  CONSTRAINT `sp_id`
    FOREIGN KEY (`sp_id`)
    REFERENCES `dhs`.`sp_details` (`sp_id`)
    ON DELETE NO ACTION
    ON UPDATE NO ACTION);

